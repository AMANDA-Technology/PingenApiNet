<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <LangVersion>12</LangVersion>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>PingenApiNet</RootNamespace>
        <Title>PingenApiNet</Title>
        <Authors>AMANDA Technology GmbH</Authors>
        <Description>Pingen API Service</Description>
        <Copyright>AMANDA Technology GmbH</Copyright>
        <PackageProjectUrl>https://github.com/AMANDA-Technology/PingenApiNet</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/AMANDA-Technology/PingenApiNet</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageId>Extended.PingenApiNet</PackageId>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\PingenApiNet.Abstractions\PingenApiNet.Abstractions.csproj" />
    </ItemGroup>

</Project>
